<%= form_for @listing do |form| %>
<%= render partial: '/listings/form', object: form %>
<% end %>

<div class="listing-wrap">
    <h1>Listing</h1>

    <% @listings.each do |l| %>
      <% l.images do |img| %>
        <%= image_tag img, class: "img-responsive" %>
      <% end %>
    <% end %>

    <%= will_paginate @listings, renderer: BootstrapPagination::Rails, previous_label: "Older", next_label: "Newer", inner_window: 1, outer_window: 0 %>
</div>
