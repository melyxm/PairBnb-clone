<h1>Edit Profile</h1>
<%= form_for(@user, html: { multipart: true}) do |f| %>
<div>
    <%= f.label :user_name %>
</div>
<div>
    <%= f.text_field :user_name %>
</div>
<div>
    <%= f.text_field :full_name %>
</div>
<div>
    <%= f.text_field :email %>
</div>
<div>
    <%= f.file_field :image %>
</div>

<%= f.submit "Save Change" %>
<% end %>
