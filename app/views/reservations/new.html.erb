<h1>Book ady</h1>

<%= form_for @reservation, url: listing_reservations_path(@listing) do |f| %>
  <%= f.label :rooms %>
  <%= f.number_field :rooms %>
  <%= f.label :no_of_guests %>
  <%= f.number_field :no_of_guests %>
  <%= f.label :check_in %>
  <%= f.date_field :check_in %>
  <%= f.label :check_out %>
  <%= f.date_field :check_out %>
<%= f.submit "Reserve" %>
<% end %>
